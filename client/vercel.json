{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/static-build" }],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/favicon.svg", "dest": "/favicon.svg" },
    { "src": "/favicon.(ico|png)", "dest": "/favicon.$1" },
    { "src": "/asset-manifest.json", "dest": "/asset-manifest.json" },
    { "src": "/manifest.json", "dest": "/manifest.json", "headers": { "cache-control": "no-cache" } },
    { "src": "/logo192.svg", "dest": "/logo192.svg" },
    { "src": "/logo512.svg", "dest": "/logo512.svg" },
    { "src": "/(.*\\.[a-zA-Z0-9]{1,10})", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "build": { "env": { "CI": "false" } }
}
